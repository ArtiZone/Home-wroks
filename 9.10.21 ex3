#include <iostream>
#include <random>

int delli(int n) {
    int count = 0;
    for (int i = 1; i < n; i++) {
        if (n % i == 0) {
            count += i;
        }
    }
    if (n == 1) {
        return 1;
    }
    return count;

}


int main()
{
    int counter = 0;
    setlocale(LC_ALL, "Russian");
    for (int i = 1; i < 10000; i++) {
        int a = delli(i);
        if (delli(a) == i) {
            counter++;
        }
        if (i % 1000 == 0) {
            std::cout << counter << " ";
        }
    }
    std::cout << "Counter is : " << counter;

}

