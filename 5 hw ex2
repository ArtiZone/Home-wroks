#include <iostream>
void print_matrix(int** a, int n, int m) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            std::cout << a[i][j] << " ";
        }
        std::cout << std::endl;
    }

}



int second_exercise(int** a, int number, int n, int m) {
    int counter = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (a[i][j] == number) {
                counter++;
            }
        }
    }
    return counter;
}

void fill_matrix(int** a, int n, int m) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            a[i][j] = rand() % 10 + 1;
        }
        std::cout << std::endl;
    }
}


int main()
{
    int n, m;
    srand(time(0));
    std::cin >> n >> m;
    int** a = new int* [n];
    for (int i = 0; i < n; i++) {
        a[i] = new int[m];
    }
    fill_matrix(a, n, m);
    print_matrix(a, n, m);
    std::cout << std::endl;
    std::cout << second_exercise(a, 5, n, m) << std::endl;
    print_matrix(a, n, m);

}
